{
  "name": "catwalk",
  "version": "2.0.0",
  "private": true,
  "description": "Nano curation tools for the humanities.",
  "main": "index.js",
  "engines": {
    "node": ">=16"
  },
  "pre-commit": [
    "lint",
    "prettier"
  ],
  "scripts": {
    "dev": "kotatsu serve ./src/main.tsx --open",
    "dts-gen": "node ./scripts/yaml-dts.js > ./i18n/types.d.ts && prettier i18n/*.d.ts --write",
    "lint": "eslint './src/**/*{.ts,.tsx,.js,.jsx}' './.storybook/*.js'",
    "prettier": "prettier src .storybook --write",
    "storybook": "start-storybook -p 6006"
  },
  "kotatsu": {
    "config": "./webpack.config.js",
    "progress": true,
    "sass": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medialab/catwalk.git"
  },
  "contributors": [
