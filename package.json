      "name": "Robin de Mourat",
      "url": "http://github.com/robindemourat"
    },
    {
      "name": "Guillaume Plique",
      "url": "http://github.com/Yomguithereal"
    },
    {
      "name": "BÃ©atrice Mazoyer",
      "url": "http://github.com/bmaz"
    }
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/medialab/catwalk/issues"
  },
  "homepage": "https://github.com/medialab/catwalk#readme",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@types/papaparse": "^5.3.5",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@yomguithereal/prettier-config": "^1.2.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "gh-pages": "^4.0.0",
    "kotatsu": "^0.23.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "storybook-react-context": "^0.6.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "papaparse": "^5.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "prettier": "@yomguithereal/prettier-config",
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "React": true
    },
    "extends": [
      "eslint:recommended",
      "eslint-config-prettier"
    ]
  }
}
