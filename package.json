{
  "name": "catwalk",
  "version": "2.0.0",
  "private": true,
  "description": "A multi-dimensional categorization tool for web & media data.",
  "main": "index.js",
  "engines": {
    "node": ">=16"
  },
  "pre-commit": [
    "check"
  ],
  "scripts": {
    "check": "npm run lint && npm run prettier:check",
    "dev": "kotatsu serve ./src/main.tsx --open",
    "dts-gen": "node ./scripts/yaml-dts.js > ./i18n/types.d.ts && prettier i18n/*.d.ts --write",
    "lint": "eslint './src/**/*{.ts,.tsx,.js,.jsx}' './.storybook/*.js'",
    "prettier:check": "prettier './src/**/*{.ts,.tsx,.js,.jsx,.scss}' .storybook --check",
    "prettier": "prettier './src/**/*{.ts,.tsx,.js,.jsx,.scss}' .storybook --write",
    "reinstall": "rimraf ./node_modules && npm i",
    "storybook": "start-storybook -p 6006"
  },
  "kotatsu": {
    "config": "./webpack.config.js",
    "public": [
      "/public",
      "./public"
    ],
    "progress": true,
    "sass": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medialab/catwalk.git"
  },
  "contributors": [
    {
      "name": "Robin de Mourat",
      "url": "http://github.com/robindemourat"
    },
    {
      "name": "Guillaume Plique",
      "url": "http://github.com/Yomguithereal"
    },
    {
      "name": "BÃ©atrice Mazoyer",
      "url": "http://github.com/bmaz"
    }
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/medialab/catwalk/issues"
  },
  "homepage": "https://github.com/medialab/catwalk#readme",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@types/file-saver": "^2.0.5",
    "@types/papaparse": "^5.3.5",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/uuid": "^8.3.4",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.0",
    "@yomguithereal/prettier-config": "^1.2.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "gh-pages": "^4.0.0",
    "kotatsu": "^0.23.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "storybook-react-context": "^0.6.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@yomguithereal/helpers": "^1.1.1",
    "classnames": "^2.3.2",
    "comma-number": "^2.1.0",
    "file-saver": "^2.0.5",
    "jotai": "^1.8.6",
    "papaparse": "^5.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-select": "^5.5.2",
    "react-social-media-embed": "^2.2.2",
    "react-virtualized-auto-sizer": "^1.0.7",
    "react-window": "^1.8.7",
    "uuid": "^9.0.0"
  },
  "prettier": "@yomguithereal/prettier-config"
}
